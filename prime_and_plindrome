//********problem1*******
//To find prime number
let num=11;

let count=0;

for(let i=1;i<=num;i++){

 if(num%i==0){

 count++;
}
}

if(count==2){

console.log("prime number");

}else{

console.log("not prime");

}

//***********problem2***********
//to find palindrome
let str="madam";

let bag="";

for(let i=0;i<str.length;i++){

   bag+=str[i];
}
if(bag==str){

console.log("palindrome");

}else{

console.log("not a palindrome");
}

